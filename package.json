{
  "name": "strong-mock",
  "version": "6.0.0",
  "description": "Simple type safe mocking library",
  "keywords": [
    "tdd",
    "mock",
    "interface",
    "typescript",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiGhTTraX/strong-mock.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": "./dist/index.esm.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "doctoc --title '**Table of Contents**' README.md",
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "microbundle --no-compress --target node -f esm,cjs --tsconfig tsconfig.build.json src/index.ts",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "lint": "eslint --ext ts,tsx,js .",
    "test": "tdd-buffet test --coverage",
    "report-coverage": "codecov"
  },
  "dependencies": {
    "jest-matcher-utils": "~27.0.0",
    "lodash": "~4.17.0"
  },
  "devDependencies": {
    "@nighttrax/eslint-config-ts": "~8.0.0",
    "@tdd-buffet/jest-config": "~4.0.0",
    "@tdd-buffet/tsconfig": "~1.0.0",
    "@types/lodash": "~4.14.0",
    "codecov": "~3.8.0",
    "doctoc": "~2.0.0",
    "eslint": "~7.28.0",
    "microbundle": "~0.13.3",
    "standard-version": "~9.3.0",
    "strip-ansi": "~6.0.0",
    "tdd-buffet": "~3.0.0",
    "typescript": "~4.3.0"
  }
}
